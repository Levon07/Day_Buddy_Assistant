<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape>
            <!-- fill/color -->
            <solid android:color="@color/NN_Black" />


            <!-- Control the width and height of the shape -->
            <size
                android:width="@dimen/_30sdp"
                android:height="@dimen/_30sdp" />

            <!-- Control the radius of each corners -->
            <corners android:radius="@dimen/_8sdp" />


        </shape>


    </item>

    <item android:drawable="@drawable/baseline_directions_walk_white_24"
        android:gravity="center"
        android:width="@dimen/_14sdp"
        android:height="@dimen/_14sdp"/>

</layer-list>